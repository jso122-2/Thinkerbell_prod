{
  "name": "thinkerbell-content-pipeline",
  "version": "1.0.0",
  "description": "Content formatting pipeline with template substitution capabilities for Thinkerbell agency",
  "main": "src/ThinkerbellPipeline.js",
  "type": "commonjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "demo": "node examples/basic-usage.js",
    "demo:templates": "node examples/template-substitution.js",
    "demo:cursor": "node examples/cursor-integration.js",
    "demo:semantic": "node examples/semantic-demo.js",
    "semantic:interactive": "node -e \"require('./examples/semantic-demo.js').interactiveDemo()\"",
    "semantic:backend": "node -e \"require('./examples/semantic-demo.js').testPythonBackend()\"",
    "api:start": "node api/semantic-bridge.js",
    "api:dev": "nodemon api/semantic-bridge.js",
    "demo:complete": "node examples/complete-integration-demo.js",
    "showcase": "npm run demo:complete",
    "setup": "npm install && echo '🎭 Thinkerbell pipeline ready!'",
    "webapp:install": "cd webapp && npm install",
    "webapp:start": "cd webapp && npm start",
    "webapp:start:3001": "cd webapp && cross-env PORT=3001 npm start",
    "webapp:build": "cd webapp && npm run build",
    "webapp:setup": "npm run webapp:install && echo '🌐 Webapp dependencies installed!'",
    "dev": "concurrently \"npm run api:start\" \"npm run webapp:start:3001\" --names \"API,WEBAPP\" --prefix-colors \"blue,green\"",
    "start": "npm run dev",
    "start:full": "npm run dev",
    "build:all": "npm run webapp:build && echo '📦 Full project build complete!'"
  },
  "keywords": [
    "content-pipeline",
    "template-engine",
    "brand-voice",
    "thinkerbell",
    "content-formatting",
    "marketing-automation"
  ],
  "author": "Thinkerbell Agency",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "directories": {
    "lib": "src",
    "example": "examples"
  },
  "files": [
    "src/",
    "examples/",
    "templates/",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "."
  },
  "homepage": "."
}
